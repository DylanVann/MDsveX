{
	"name": "mdsvex",
	"version": "0.8.5",
	"description": "Markdown preprocessor for Svelte",
	"main": "dist/main.cjs.js",
	"module": "dist/main.es.js",
	"browser": "dist/main.browser.js",
	"repository": "https://github.com/pngwn/MDsveX",
	"scripts": {
		"build": "rollup -c",
		"lint": "eslint . --ext .js,.ts",
		"test": "uvu -r ts-node/register . test.ts$",
		"generate_files": "node -r esm ./test/_scripts/generate_markdown_html.js",
		"types": "dts-generator --project . --out dist/main.es.d.ts"
	},
	"files": [
		"dist/*"
	],
	"keywords": [
		"test",
		"preprocessor",
		"mdx",
		"markdown",
		"svelte"
	],
	"author": "pngwn <hello@pngwn.io>",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^14.0.27",
		"@typescript-eslint/eslint-plugin": "^3.9.0",
		"@typescript-eslint/parser": "^3.9.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-plugin-prettier": "^3.1.2",
		"esm": "^3.2.25",
		"prettier": "^2.0.5",
		"prettier-eslint": "^9.0.0",
		"ts-node": "^8.10.2",
		"uvu": "^0.0.14"
	}
}
